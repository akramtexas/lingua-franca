// Periodically produce an output consisting of a count followed by
// ": Hello World!".
language JavaScript;
actor Source(period:int(1000)){
    trigger t(period, PERIODIC);
    output y:int;
    preamble {
        var n = 0;
    }
    initialize {
        n = 0;
    }
    reaction(t) -> y {
        n = n + 1;
        set(y, n);
    }
}

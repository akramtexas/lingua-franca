// Test the lexer with various supported syntaxes.
target JavaScript;
actor TestLexer(
        period:int(1000),
        foo:string("hello"),
        bar:foreignType({= @#$% =})
    ) {
    trigger t(period, PERIODIC);
    output y:int;
    preamble {=
        var n = 0;
    =}
    initialize {=
        n = 0;
    =}
    reaction(t) -> y {=
        n = n + 1;
        set(y, n);
    =}
}

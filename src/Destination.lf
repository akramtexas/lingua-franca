// Actor that displays an input string.
language JavaScript;
actor Destination {
    inputs x:string;
    preamble {
        var textDisplay = require('@accessors-modules/text-display');
        var display = null;
    }
    initialize {
        if (display === null) {
            display = new textDisplay.TextDisplay(this.getParameter('title'));
        }
    }
    reaction(x) {
        display.appendText(get(x));
    }
}
